    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSetCidadesEstados">
      <Query>
        <DataSourceName>DataSourceCidades</DataSourceName>
        <CommandText>select 
	E.SG_ESTADO, 
	C.NM_CIDADE 
from 
	TB_CIDADE as C 
	inner join TB_ESTADO as E on 
	C.CD_ESTADO = E.CD_ESTADO 
WHERE 
		C.BT_REMOVIDO = 0 
	AND E.BT_REMOVIDO = 0 
               
order by 
	E.SG_ESTADO, 
	C.NM_CIDADE </CommandText>
      </Query>
      <Fields>
        <Field Name="SG_ESTADO">
          <DataField>SG_ESTADO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NM_CIDADE">
          <DataField>NM_CIDADE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
